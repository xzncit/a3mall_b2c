<view><view class="{{(('address-action')+' '+((value==true)?'address-show':''))}}" style="background-color:#f8f8f8;"><view class="address-title">请选择地址</view><view class="address-body" style="{{'max-height:'+(maxHeight+'px')+';'}}"><block a:if="{{array.length<=0}}"><view class="address-empty">{{''+tips+''}}</view></block><block a:if="{{array.length}}"><view class="address-list"><view data-event-opts="{{[['tap',[['onSelect',['$0'],[[['array','',index]]]]]]]}}" class="address-box" onTap="__e" a:for="{{array}}" a:for-item="item" a:for-index="index" a:key="index"><view class="address-r-box"><view class="address-name">{{item.name+" "+item.tel}}</view><view class="address-valid">{{item.address}}</view></view><view class="address-corner-checkbox"><text class="{{(('iconfont')+' '+((active==item.id)?'active':''))}}"></text></view></view></view></block><view style="width:100%;height:60px;float:left;"></view></view><text class="fa fa-times-circle" id="close" data-event-opts="{{[['tap',[['onClose',['$event']]]]]}}" catchTap="__e"></text><block a:if="{{add}}"><view data-event-opts="{{[['tap',[['onAddAddress',['$event']]]]]}}" class="address-button" onTap="__e"><text>新增地址</text></view></block></view><popup onInput="__e" vue-id="456bab10-1" value="{{value}}" data-event-opts="{{[['^input',[['__set_model',['','value','$event',[]]]]]]}}" onVueInit="__l"></popup></view>