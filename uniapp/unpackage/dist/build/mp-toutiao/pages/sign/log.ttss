.list-wrap{margin-top:20rpx}.list-wrap .point-list{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-flex-wrap:wrap;flex-wrap:wrap;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;font-size:28rpx}.list-wrap .point-list .point-item{width:95%;margin:0 auto;margin-bottom:20rpx;border-radius:12rpx;height:auto !important;height:260rpx;min-height:260rpx;background-color:#fff;border-bottom:2rpx solid #eee}.list-wrap .point-list .point-item .t{height:80rpx;line-height:80rpx;border-bottom:2rpx solid #eee}.list-wrap .point-list .point-item .t text:first-child{float:left;padding-left:20rpx}.list-wrap .point-list .point-item .t text:last-child{float:right;padding-right:20rpx}.list-wrap .point-list .point-item .m{padding:10rpx 20rpx;display:block;height:auto !important;height:100rpx;min-height:100rpx}.list-wrap .point-list .point-item .b{height:60rpx;line-height:60rpx;border-top:2rpx solid #eee}.list-wrap .point-list .point-item .b text{float:right;margin-right:20rpx}