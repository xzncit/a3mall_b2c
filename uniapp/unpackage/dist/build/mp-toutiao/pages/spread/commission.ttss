.list-wrap{width:100%;margin-top:20rpx}.list-wrap .list-item{width:92%;height:auto !important;height:160rpx;min-height:160rpx;border-radius:10rpx;background-color:#fff;margin:0 auto;margin-bottom:20rpx;font-size:26rpx}.list-wrap .list-item .item-box{height:auto !important;height:160rpx;min-height:160rpx;position:relative}.list-wrap .list-item .item-box .ll{float:left;width:55%}.list-wrap .list-item .item-box .ll text{display:block;padding-left:5%;float:left}.list-wrap .list-item .item-box .ll text:first-child{font-size:26rpx;color:#333;margin-top:30rpx}.list-wrap .list-item .item-box .ll text:last-child{font-size:24rpx;color:#999;margin-top:10rpx}.list-wrap .list-item .item-box .rr{float:right;width:35%;height:160rpx;line-height:160rpx;text-align:right;padding-right:5%;color:#fc4141;font-size:32rpx}