<view class="{{(('aaa-count-down')+' '+theme)}}"><block a:if="{{isShow}}"><view class="{{('wrap-'+theme)}}"><block a:if="{{before}}"><view class="before">{{before}}</view></block><block a:if="{{time.day>0}}"><view class="day"><text>{{time.day}}</text><text>{{dayText}}</text></view></block><view class="hour"><text>{{time.hour}}</text><text>{{hourText}}</text></view><view class="minute"><text>{{time.minute}}</text><text>{{minuteText}}</text></view><view class="second"><text>{{time.second}}</text><text>{{secondText}}</text></view><block a:if="{{after}}"><view class="after">{{after}}</view></block></view></block><block a:if="{{!isShow}}"><view class="{{('tips-'+theme)}}">{{tips}}</view></block></view>