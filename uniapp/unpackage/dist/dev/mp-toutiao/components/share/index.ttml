<view class="{{(('share-box data-v-466363b7')+' '+((value)?'share-show':''))}}"><view class="share-gride data-v-466363b7"><block tt:for="{{providerList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><block tt:if="{{providerList.length>0}}"><view data-event-opts="{{[['tap',[['share',['$0'],[[['providerList','',index]]]]]]]}}" class="gride-box data-v-466363b7" bindtap="__e"><image src="{{item.img}}" class="data-v-466363b7"></image><view class="data-v-466363b7">{{item.name}}</view></view></block></block><block tt:if="{{providerList.length<=0}}"><view class="share-empty data-v-466363b7">您还未开通相关服务</view></block></view><view class="iconfont data-v-466363b7" id="close" data-event-opts="{{[['tap',[['onClose',['$event']]]]]}}" catchtap="__e">取消分享</view></view>